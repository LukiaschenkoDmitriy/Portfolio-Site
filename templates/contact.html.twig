<div class="wrapp-contact-container">
    <div class="container">
        <div class="contact-container">
            {% for contact in contacts %}
                {% set preffix = "" %}
                {% if (contact.name == "Phone Ukraine" or contact.name == "Phone Polish") %}
                    {% set preffix = "tel:" %}
                {% endif %}
                <div class="contact">
                    <a href="{{preffix}}{{contact.contact}}" class="contact-link">
                        <img src="{{contact.icon_path}}" alt="{{contact.name}}">
                    </a>
                </div>
            {% endfor %}
        </div>
    </div>
</div>